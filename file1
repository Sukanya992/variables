pipeline{
   ageny any {
     environment {
     name='abc'
     age=10
     }
     
       stages{
           stage ('Build') {
                  steps {
                  
                 sh 'echo Building application= ${env.name}'
                 sh 'echo Building application= ${env.age}'
                 
                 }
                 }
              }
              }
          }
          
